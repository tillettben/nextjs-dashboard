# Environment Variables Template
# Copy this file to .env.local for local development

# Local Database (Docker)
DATABASE_URL_LOCAL=postgres://dev_user:dev_password@localhost:5433/nextjs_dashboard

# Test Database (Local)
DATABASE_URL_TEST=postgres://dev_user:dev_password@localhost:5433/nextjs_dashboard_test

# Production Database (set in Vercel)
POSTGRES_URL=your_production_database_url_here

# NextAuth Configuration
AUTH_SECRET=your_auth_secret_here
AUTH_URL=http://localhost:3000

# Environment
NODE_ENV=development